    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Build') {
            steps {
                echo 'BUilding Develop'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying Develop'
            }
        }   
        stage('test') {
            steps {
                echo 'Testing Deploy'
            }
        }
    }
}
